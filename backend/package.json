{
  "name": "rental-backend",
  "version": "1.0.0",
  "description": "High-performance rental management system backend with comprehensive security and validation",
  "main": "src/server.js",
  "scripts": {
    "start": "set NODE_ENV=production && node src/server.js",
    "dev": "set NODE_ENV=development && nodemon src/server.js --watch src --ignore src/logs/",
    "seed": "node scripts/seed.js",
    "seed:dev": "set NODE_ENV=development && node scripts/seed.js",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "validate": "npm run lint && npm run test",
    "clean": "rimraf node_modules dist coverage",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.11.2",
    "express-validator": "^7.2.1",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.18.0",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "eslint": "^8.47.0",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "rental",
    "api",
    "express",
    "mongodb",
    "performance",
    "security",
    "validation",
    "rate-limiting",
    "caching",
    "transactions"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/rental-backend.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/rental-backend/issues"
  },
  "homepage": "https://github.com/your-username/rental-backend#readme"
}
